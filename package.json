{
  "name": "@formkit/drag-and-drop",
  "version": "0.0.11",
  "private": true,
  "description": "Drag and drop package.",
  "main": "./index.cjs",
  "types": "./index.d.cts",
  "type": "module",
  "scripts": {
    "test": "npx playwright test vue",
    "docs": "cd docs && pnpm run dev",
    "dev": "cd dev && pnpm run dev",
    "build": "jiti build.ts",
    "release": "npx bumpp --no-tag --no-push --no-commit && pnpm run build && cd dist && npx publint && pnpm publish",
    "lint": "eslint ."
  },
  "packages": [],
  "keywords": [],
  "author": "Sasha Milenkovic <sasha@formkit.com>",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "bumpp": "^9.2.0",
    "esbuild-plugin-replace": "^1.4.0",
    "eslint": "^8.55.0",
    "jiti": "^1.21.0",
    "pathe": "^1.1.2",
    "publint": "^0.2.7",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "exports": {
    "./vue": {
      "require": {
        "types": "./vue/index.d.cts",
        "default": "./vue/index.cjs"
      },
      "import": {
        "types": "./vue/index.d.ts",
        "default": "./vue/index.mjs"
      }
    },
    "./react": {
      "require": {
        "types": "./react/index.d.cts",
        "default": "./react/index.cjs"
      },
      "import": {
        "types": "./react/index.d.ts",
        "default": "./react/index.mjs"
      }
    },
    ".": {
      "require": {
        "types": "./index.d.cts",
        "default": "./index.cjs"
      },
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  }
}
